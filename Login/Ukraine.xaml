<Window x:Class="Login.Ukraine"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login"
        mc:Ignorable="d"
        Title="Ukraine" Height="400" Width="800">
    <DockPanel>
        <DockPanel.Background>
            <ImageBrush ImageSource="Resources/Ukraine.jpg"/>
        </DockPanel.Background>
        <TextBlock Text="{DynamicResource Header}" TextAlignment="Center" FontSize="35"  Foreground="Yellow" FontWeight="Bold" DockPanel.Dock="Top"/>
        <TextBlock Text="{DynamicResource Footer}" TextAlignment="Center"  Foreground="Blue" FontWeight="Bold" DockPanel.Dock="Bottom" FontSize="35" Height="40"/>
        <Button x:Name="Return"  Content="Return to Login" Background="Yellow" Foreground="Blue" FontSize="25" FontWeight="Bold" Height="40" Width="200" DockPanel.Dock="Bottom" Click="Return_Click"/>
    </DockPanel>
</Window>
